{
    "name": "Ontab for Notion",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Sample Chrome Extension",
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "https://api.notion.com/"
    ],
    "action": {
        "default_icon": {
            "16": "icon.png"
        },
        "default_title": "Send tabs to Notion",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
