{
    "name": "Send tabs to Notion",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Send tabs to Notion",
    "permissions": [
        "tabs",
        "storage",
        "identity"
    ],
    "host_permissions": [
        "https://api.notion.com/"
    ],
    "action": {
        "default_icon": {
            "16": "icon.png"
        },
        "default_title": "Send tabs to Notion",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    }
}
